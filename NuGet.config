<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<packageSources>
		<add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
		<add key="optimizely nuget.org" value="https://api.nuget.optimizely.com/v3/index.json" />
		<add key="episerver-github" value="https://nuget.pkg.github.com/episerver/index.json" />
	</packageSources>
	<packageRestore>
		<add key="enabled" value="True" />
		<add key="automatic" value="True" />
	</packageRestore>
	<bindingRedirects>
		<add key="skip" value="False" />
	</bindingRedirects>
	<packageManagement>
		<add key="disabled" value="False" />
		<add key="format" value="0" />
	</packageManagement>
	<packageSourceCredentials>
		<episerver-github>
			<add key="Username" value="Ignored" />
			<add key="ClearTextPassword" value="%GitHubEpiserverPAT%" />
		</episerver-github>
	</packageSourceCredentials>
</configuration>